
generator client {
    provider = "prisma-client-js"
}


datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
    id String @id @default(auto()) @map("_id") @db.ObjectId // MongoDB specific ID field
  
    email String @unique
    username String @unique
    password String
    avatar String? // Optional field
    createdAt DateTime @default(now())


}